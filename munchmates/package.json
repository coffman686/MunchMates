{
  "name": "munchmates",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "kc:up+logs": "docker compose -f login/keycloak/docker-compose.yml up -d --wait && docker compose -f login/keycloak/docker-compose.yml logs -f",
    "dev:all": "npx concurrently --kill-others-on-fail -n kc,next -c blue,green \"npm run kc:up+logs\" \"npx wait-on tcp:8080 && npm run dev\"",
    "kc:down": "docker compose -f login/keycloak/docker-compose.yml down",
    "kc:nuke": "docker compose -f login/keycloak/docker-compose.yml down -v",
    "kc:logs": "docker compose -f login/keycloak/docker-compose.yml logs -f",
    "dev:all:detached": "npm run kc:up && npx wait-on http://localhost:8080 && next dev -p 3000",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@keycloak/keycloak-admin-client": "^26.4.7",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docker": "^1.0.0",
    "jose": "^6.1.0",
    "keycloak-js": "^26.2.1",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "pg": "^8.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "shadcn": "^3.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
